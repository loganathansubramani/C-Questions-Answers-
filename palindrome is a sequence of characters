#include<stdio.h>
#include<string.h>
int checkPalindrome(char s[],int i,int j)
{
while(i<j)
{
if(s[i] != s[j])
return 0;
i++;
j--;
}
return 1;
}
int main()
{
char inp[30];
scanf("%s", inp);
int n=strlen(inp);
int i;
for(i=0;i<n;i++)
{
if(checkPalindrome(inp,i,n-1))
break;
}
for(int j = i-1; j >= 0; j--)
printf("%c", inp[j]);
return 0;
}
